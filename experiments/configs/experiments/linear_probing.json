{
  "device": "cuda",
  "criterion": "CrossEntropyLoss()",
  "batch_size": 8,
  "lr": 0.0001,
  "num_epochs": 10,
  "optimizer": "torch.optim.adamw.AdamW",
  "weight_decay": 0,
  "dropout_rate": 0.1,
  "early_stopping": { "enabled": "False", "patience": 3, "delta": 0 },
  "gradient_clipping": { "enabled": "False", "max_norm": 0 },
  "scheduler": {
    "enabled": "False",
    "warmup_epochs": 0,
    "eta_min": 0,
    "linear_start_factor": 0.1,
    "linear_end_factor": 1.0
  }
}
